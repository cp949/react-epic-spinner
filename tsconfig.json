{
  "compilerOptions": {
    "declaration": true,
    "target": "es5",
    "lib": ["dom",  "es6"],
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "typeRoots": ["./typings"],
    // "noEmit": true,
    "jsx": "react"
  },
  "include":["src/**/*"]
}
